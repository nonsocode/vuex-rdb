{
  "name": "vuex-rdb",
  "version": "0.26.0",
  "description": "An Active record database for vuex",
  "main": "dist/vuex-rdb.cjs.js",
  "typings": "types/index.d.ts",
  "browser": "dist/vuex-rdb.js",
  "module": "dist/vuex-rdb.esm.js",
  "unpkg": "dist/vuex-rdb.min.js",
  "repository": "https://github.com/nonsocode/vuex-rdb.git",
  "author": "Chinonso Chukwuogor<nonsochukwuogor@gmail.com>",
  "license": "MIT",
  "private": false,
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --ignore-path=.prettierignore"
    }
  },
  "files": [
    "dist",
    "types"
