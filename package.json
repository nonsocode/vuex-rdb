  "peerDependencies": {
    "vue": "^2.6.0",
    "vuex": "^3.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-replace": "^2.3.1",
    "@rollup/plugin-typescript": "^5.0.2",
    "@types/jest": "^26.0.15",
    "core-js": "^2.6.11",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "reflect-metadata": "^0.1.13",
    "rollup": "^2.3.2",
    "rollup-plugin-terser": "^5.3.0",
    "ts-jest": "^26.4.4",
    "tslib": "^2.0.1",
    "typescript": "^4.0.0",
    "vue": "^2.6.12",
    "vuex": "^3.5.1"
  },
  "dependencies": {}
